export ELECTRON=$HOME/.electron
export APPLICATIONS_FOLDER=$ELECTRON/applications
export ELECTRON_CONFIGRC=$ELECTRON/configrc
export ELECTRON_SECRET=$HOME/Dropbox/electron

source "$ELECTRON/config.sh"

# passwordstore
export  PASSWORD_STORE_DIR=$ELECTRON_SECRET/passwordstore
# automated additions ---

# Node.js 18.16.0
export PATH=$PATH:$APPLICATIONS_FOLDER/node-v18.16.0/bin
# JDK 20
export PATH=$PATH:/home/jayam/.electron/applications/jdk-20.0.1/bin
# firebase tools linux
export PATH=$PATH:/home/jayam/.electron/applications/firebase-tools-linux
